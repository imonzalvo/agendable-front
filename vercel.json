{
    "rewrites": [
        {
            "source": "/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "(?<subdomain>.*).agendable.io"
                }
            ],
            "destination": "/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/a/:subdomain/:path*"
        },
        {
            "source": "/app",
            "destination": "/"
        },
        {
            "source": "/app/:match*",
            "destination": "/:match*"
        },
        {
            "source": "/app/:path*",
            "has": [
                {
                    "type": "host",
                    "value": "(?<subdomain>.*).agendable.io"
                }
            ],
            "destination": "/:path*"
        }
    ],
    "redirects": [
        {
            "source": "/app",
            "destination": "/"
        },
        {
            "source": "/app/:match*",
            "destination": "/:match*"
        }
    ]
}